{#
    Available variables:

    Metadata metadata     - metadata of merge
    mixed convertedValue  - value converted to string
    mixed value           - original value
    boolean isListValue   - is value represents list
#}
{% if isListValue %}
    <ul>
    {% for value in convertedValue %}
        <li>{{ value }}</li>
    {% endfor %}
    </ul>
{% else %}
    {{ convertedValue }}
{% endif %}
